  postgres_db:
    environment:
      POSTGRES_DB: anilist_db
      POSTGRES_PASSWORD: mya
      POSTGRES_USER: neko
    image: postgres:latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5434"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: pgdata
      target: /var/lib/postgresql/data
